{
  "name": "transcribe-function",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "unit-test": "mocha test/index.test.js test/*unit*test.js test/*integration*test.js --timeout=2000 --exit",
    "system-test": "mocha test/*system*test.js --timeout=600000 --exit",
    "all-test": "npm run unit-test && npm run system-test",
    "test": "npm -- run all-test",
    "deploy": "gcloud functions deploy transcribeFile --runtime nodejs10 --trigger-resource transcriber_files --trigger-event google.storage.object.finalize",
    "trigger-upload": "gsutil cp gcf-test.txt gs://transcriber_files"
  },
  "dependencies": {
    "@google-cloud/debug-agent": "^5.0.0",
    "@google-cloud/speech": "^4.1.5",
    "@google-cloud/storage": "^5.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^9.1.0",
    "music-metadata": "^7.6.4"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.1.1",
    "@google-cloud/pubsub": "^2.0.0",
    "child-process-promise": "^2.2.1",
    "express": "^4.16.3",
    "mocha": "^8.0.0",
    "moment": "^2.24.0",
    "promise-retry": "^2.0.0",
    "proxyquire": "^2.1.0",
    "request": "^2.88.0",
    "requestretry": "^4.0.0",
    "sinon": "^9.0.0",
    "supertest": "^6.0.0",
    "uuid": "^8.0.0"
  }
}
